
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

model Player {
  id          String   @id @default(cuid())
  name        String
  score       Int
  lastUpdated DateTime @updatedAt
}
